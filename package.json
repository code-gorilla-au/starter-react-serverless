{
	"name": "starter-react",
	"private": true,
	"type": "module",
	"engines": {
		"node": ">=24.12.0"
	},
	"scripts": {
		"build": "react-router build",
		"dev": "react-router dev",
		"typegen": "react-router typegen",
		"format": "prettier --write .",
		"test:watch": "vitest --coverage",
		"test:unit": "vitest --run --coverage",
